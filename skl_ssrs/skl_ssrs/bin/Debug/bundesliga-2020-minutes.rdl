  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Competition.competition_id, Game.season, Fact.minutes_played, Player.name, Club.club_code
FROM     Competition INNER JOIN
                  Fact ON Competition.competition_id = Fact.competition_id INNER JOIN
                  Game ON Fact.game_id = Game.game_id INNER JOIN
                  Player ON Fact.player_id = Player.player_id INNER JOIN
                  Club ON Fact.club_id = Club.club_id AND Player.current_club_id = Club.club_id
WHERE  (Game.season = '2020') AND (Competition.competition_id = 'L1') AND (Club.club_code = 'borussia-dortmund')</CommandText>
      </Query>
      <Fields>
        <Field Name="competition_id">
          <DataField>competition_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="season">
