          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="minutes_played">
          <DataField>minutes_played</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="club_code">
          <DataField>club_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
