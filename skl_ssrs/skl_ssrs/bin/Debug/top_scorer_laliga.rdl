  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Player.name, Game.season, Fact.goals
FROM     Club INNER JOIN
                  Fact ON Club.club_id = Fact.club_id INNER JOIN
                  Competition ON Fact.competition_id = Competition.competition_id INNER JOIN
                  Game ON Fact.game_id = Game.game_id INNER JOIN
                  Player ON Club.club_id = Player.current_club_id AND Fact.player_id = Player.player_id
WHERE  (Competition.competition_id = 'ES1')
ORDER BY Fact.goals</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="season">
          <DataField>season</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="goals">
          <DataField>goals</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
